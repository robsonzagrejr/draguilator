def factorial(int i)
{
    int ret;
    ret = 1;
    if (i < 0)
    {
        string errorMessage;
        errorMessage = "Invalid input to the factorial function";
        print errorMessage;
        return;
    }
    int k;
    int j;
    for (j = 1; j < i; j = j + 1)
    {
        k = j + 1;
        ret = ret * k;
    }
    string successMessage;
    string ks;
    string is; 
    is = i;
    ks = k;
    successMessage = "The factorial of number " + is;
    successMessage = successMessage + " is ";
    successMessage = successMessage + ks;
    print successMessage;
    return;
}

def getRandomNumber(int seed, int range)
{
    int a;
    int b;
    int c;
    int d;
    a = 1664525;
    b = 1013904223;
    c = 4294967296;
    d = 1048576;
    seed = seed * a;
    seed = seed + b;
    seed = seed % c;
    int randomNumber;
    randomNumber = seed / d;
    randomNumber = randomNumber % range;
    string returnMessage;
    returnMessage = "Your random number is ";
    returnMessage = returnMessage + randomNumber;
    print randomNumber;
    return;
}

