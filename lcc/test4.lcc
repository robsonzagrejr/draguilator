def max(int x, int y)
{
    string returnMessage;
    if (x > y)
    {
        returnMessage = x + "is greater than " + y;
        return returnMessage;
    }

    returnMessage = y + "is greater than " + x;
    return returnMessage;
}

def min(int x, int y)
{
    string returnMessage;
    if (x > y)
    {
        returnMessage = x + "is greater than " + y;
        return y;
    }

    returnMessage = y + "is greater than " + x;
    return x;
}

max(a, b);
max(b, c);
max(a, c);
min(a, b);
min(b, c);
min(a, c);

def iterativeSum(int a, int b)
{
    for (int i = 0; i < b; b = b + 1)
    {
        a = a + 1;
    }

    string returnMessage;
    returnMessage = "The result is: ";
    returnMessage = returnMessage + a;
    return returnMessage;
}

iterativeSum(a, b);
iterativeSum(a, c);
iterativeSum(b, c);

